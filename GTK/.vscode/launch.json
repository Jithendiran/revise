{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Gnome Debugger connect with serevr",
      "type": "cppdbg",
      "request": "launch",
      "program": "/tmp/build-tmp/install/bin/gnome-system-monitor",
      "miDebuggerServerAddress": ":12345",
      "miDebuggerPath": "/usr/bin/gdb",
      "cwd": "${workspaceFolder}",
      "MIMode": "gdb",
      "logging": {
        "engineLogging": true
      },
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "postRemoteConnectCommands": [
        {
          "description": "Set source directory",
          "text": "dir ${workspaceFolder}/gnome-system-monitor/src"
        },
        {
          "description": "Set source for lib",
          "text": "set substitute-path /run/build /media/ssd/Project/open_source/gnome-monitor/build-dir/files/lib/debug/source"
        }
      ],
      "stopAtEntry": false
    },
    {
      "name": "Gnome Debugger",
      "type": "cppdbg",
      "request": "launch",
      "program": "/tmp/build-tmp/install/bin/gnome-system-monitor",
      "miDebuggerPath": "/usr/bin/gdb",
      "cwd": "${workspaceFolder}",
      "MIMode": "gdb",
      "environment": [
        {
          "name": "GSETTINGS_SCHEMA_DIR",
          "value": "/tmp/build-tmp/install/share/glib-2.0/schemas"
        },
        {
          "name": "G_DEBUG",
          "value": "fatal-criticals"
        }
      ],
      "logging": {
        "engineLogging": true
      },
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ],
      "postRemoteConnectCommands": [
        {
          "description": "Set source directory",
          "text": "dir ${workspaceFolder}/gnome-system-monitor/src"
        },
        {
          "description": "Set source for lib",
          "text": "set substitute-path /run/build /media/ssd/Project/open_source/gnome-monitor/build-dir/files/lib/debug/source"
        }
      ],
      "stopAtEntry": false
    },
        {
